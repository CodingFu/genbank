!!!
%html
  %head
    %title Bank
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    = render 'common/nav'
    .container
      .row
        - if user_signed_in?
          .span7
            = yield
          .span5
            = render 'common/accounts'
        - else
          .span12
            = yield
